{
  "name": "vue-core-composables",
  "version": "0.0.1",
  "description": "Vue core composables",
  "main": "./dist/vue-core-composables.umd.js",
  "module": "./dist/vue-core-composables.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-core-composables.mjs",
      "require": "./dist/vue-core-composables.umd.js"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@types/node": "^22.13.14",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.0.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  },
  "keywords": [
    "vue",
    "vue3",
    "composables",
    "library"
  ],
  "author": "Ernesto Canquiz",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.5.13"
  }
}
